<UserControl x:Class="Chem4Word.UI.TwoD.SelectionRotationTool" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Height="300" Width="300">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="15" />
			<RowDefinition Height="30" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>
		<Ellipse x:Name="circle" Fill="Green" HorizontalAlignment="Center" Height="15" Width="15" Stroke="Black" StrokeThickness="1" />
		<Border x:Name="stick" HorizontalAlignment="Center" Width="1" BorderThickness="1" BorderBrush="Black" Grid.Row="1" IsHitTestVisible="False" />
		<Border Name="border1" Grid.Row="2" CornerRadius="15,15,15,15" BorderThickness="1" BorderBrush="Black" IsHitTestVisible="False" />
	</Grid>
</UserControl>