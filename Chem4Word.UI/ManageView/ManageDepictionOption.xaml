<Window x:Class="Chem4Word.UI.ManageView.ManageViewOption" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="Manage View Option" Width="371" Height="372" WindowStartupLocation="CenterScreen" Topmost="True" ResizeMode="CanResize" MaxWidth="333" MinWidth="371" ShowInTaskbar="False">
	<Grid Margin="0,0,0,0">
		<Grid.RowDefinitions>
			<RowDefinition Height="30" />
			<RowDefinition Height="25" />
			<RowDefinition Height="274*" />
			<RowDefinition Height="45" />
		</Grid.RowDefinitions>
		<TextBlock Name="Explanation" Background="White" Width="Auto" Height="30" FontSize="14" Grid.Row="0" Margin="10,0,10,0"></TextBlock>
		<TextBlock Text="Representations Available" Background="White" Width="Auto" FontSize="14" Grid.Row="1" Margin="10,0,10,0" />
		<ScrollViewer Grid.Row="2" Margin="0,0,0,0" VerticalScrollBarVisibility="Auto">
			<StackPanel Name="DepictionsStack" Orientation="Vertical" Background="White" Margin="10,0,0,0"></StackPanel>
		</ScrollViewer>
		<Button Name="okButton" Margin="0,0,133,10" Grid.Row="3" IsDefault="True" Height="29" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="92">OK</Button>
		<Button Name="cancelButton" Margin="0,0,20,10" Grid.Row="3" HorizontalAlignment="Right" Width="92" IsCancel="True" Height="29" VerticalAlignment="Bottom">Cancel</Button>
	</Grid>
</Window>